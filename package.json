  "license": "Apache-2.0",
  "keywords": [
    "mina-zkapp",
    "mina-zk-app",
    "mina-dapp",
    "zkapp"
  ],
  "type": "module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "scripts": {
    "demo": "npx tsc -p tsconfig.json && node --experimental-specifier-resolution=node ./build/src/demo.js",
    "demo:operator": "npx tsc -p tsconfig.json && node --experimental-specifier-resolution=node ./build/src/rollup_operator/index.js",
    "demo:client": "npx tsc -p tsconfig.json && node --experimental-specifier-resolution=node ./build/src/client_sdk/index.js",
    "build": "tsc -p tsconfig.json",
    "coverage": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js --coverage --config jest.config.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-modules --experimental-wasm-threads --no-warnings node_modules/jest/bin/jest.js --watch",
    "lint": "npx eslint src/* --fix"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "abstract-level": "^1.0.3",
    "base-x": "^4.0.0",
    "body-parser": "^1.20.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.0.1",
    "express": "^4.17.3",
    "level": "^8.0.0",
    "mina-signer": "^1.1.0",
    "pg": "^8.7.3",
    "pino": "^8.0.0",
    "pino-pretty": "^8.0.0",
    "snarkyjs": "^0.4.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.3",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.7.0",
    "eslint-plugin-snarkyjs": "^0.3.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.5.2"
  }
